if [ "${versionFBLR:=0.0.0}" == "0.0.0" ] ; then 
 
 unset FibBinaryLevelRepr FBLRDisplayDebug FBLRDisplayEntry FBLRDisplayProof Arg0 ; 
 #eval $( GFCFuncName=FibBinaryLevelRepr GlobalFunctionCleaning ) ; 

 function FibBinaryLevelRepr()
 {
  local -a ArrayArg=( $* ) ;
  local -a __call_locality=( FBLR FibBinaryLevelRepr ) ;
  local Arg0=${ArrayArg[0]} ;   
  
  local BoolDisplayDebug=${FBLRDisplayDebug:=False} ; 
  local BoolDisplayEntry=${FBLRDisplayEntry:=False} ; 
  local BoolDisplayProof=${FBLRDisplayProof:=False} ; 
  
  local -a ArrayMsg ; 
  ArrayMsg[0]="Representation over __NUMBER__ binary-position, of number __INT__\n" ; 
  ArrayMsg[1]="Number: __NUMBER__\nFib Harmonic: [__INT__]\nBinary Level Repr:[__REPR__]\n" ; 

  
  local StrReprFibLevel ;
  local IntOldLevel=0 ;
  local StrResultScaled=$( eval \
           GVSFHDisplayDebug=False \
           GVSFHDisplayEntry=False \
           GVSFHDisplayProof=False \
           GVSFHIntValue=${Arg0} \
           GetValueScaledFibHarmonic  ) ; 
  local -a ArrayFibHarmonic=( ${StrResultScaled} ) ; 
  
  IntLevelNb=$(( ${#ArrayFibHarmonic[@]} )) ;  
  IntLevel=$(( ${IntLevelNb} - 1 )) ;
    
  StrMsg=${ArrayMsg[0]} ;
  StrMsg=${StrMsg//__NUMBER__/${ArrayFibHarmonic[0]}}
  StrMsg=${StrMsg//__INT__/${Arg0}} 
  if [ "${BoolDisplayDebug:=False}" == "True" ] ; then 
    echo -ne "${StrMsg}" > /dev/stderr ;
  fi 
  
  for (( intx=1 ; intx <= ${ArrayFibHarmonic[0]} ; intx++ )) ; do 
   IntLimitFix=${ArrayFibHarmonic[${IntLevel}]} ;
   if [ ${intx} -eq ${IntLimitFix} ] ; then 
    StrReprFibLevel="1${StrReprFibLevel}" ; 
    IntLevel=$(( ${IntLevel} - 1 )) ; 
   else
    StrReprFibLevel="0${StrReprFibLevel}" ;
   fi 
  done 
  
  StrMsg=${ArrayMsg[1]} ;
  StrMsg=${StrMsg//__NUMBER__/${Arg0}} ; 
  StrMsg=${StrMsg//__INT__/${ArrayFibHarmonic[@]}} ; 
  StrMsg=${StrMsg//__REPR__/${StrReprFibLevel}} ;  
  
  if [ "${BoolDisplayDebug:=False}" == "True" ] ; then 
   echo -ne "${StrMsg}" > /dev/stderr ;
  fi 
  echo "${StrReprFibLevel}" ; 
  
  
 }
fi

if [ "${versionFBLR:=0.0.1}" == "0.0.1" ] ; then 
 eval $( GFCFuncName=FibBinaryLevelRepr GlobalFunctionCleaning ) ; 
 function FibBinaryLevelRepr()
 {
  function Verbosis()
  {
   local -a ArrayArg=( $* ) ;
   local -a __call_locality=( Verb Verbosis ) ; 
   local Arg0=${ArrayArg[0]} ; 
   local StrMsg=${VerbMsg:=__TEXT__} ; 
   local StrDevOut=${VerbDev:=/dev/stderr} ; 
   local IsVerboseState=${VerbState:=False} ;
